        <style>
            :root{
    --back_grande: #131726;
    --back_caixa: rgb(6, 11, 20);
    --form_color: black;
    --form_border: #686868;
    --form_text: #cef3f5;
    --link_color: rgb(19, 159, 214);
}

#logo2{
    height: 60px;
    padding-right: 20px;
    background-color: var(--back_grande);
}

#logo2:hover{
    cursor: pointer;
    background-color: var(--back_grande);
}

#menus{
    list-style-type: none;
    overflow: hidden;
    background-color: var(--back_grande);
    padding-left: 32px;
}

#menus li{
    float: left;
    height: 60px;
    padding: 55px;
    padding-top: 20px;
    padding-bottom: 20px;
    transition: 0.3s;
    color: var(--form_text);
    font-size: 16px;
    font-weight: 400 !important;
}

#menus li:hover{
    text-decoration: none;
    background-color: var(--form_border);
    cursor: pointer;
}

*{
    margin: 0;
    padding: 0;
}

body{
    background-image: linear-gradient(to right, #0f2027, #12252c,
     #152931, #192e37, #1c333c,
      #1e3741, #203c47, #22404c,
       #244553, #274959, #294e60, #2c5367) !important; 
    height: 100%;
    width: 100%;
}

.nav_tabs{
    width: 1100px;
    height: 800px;
    margin: 30px auto;
    position: relative; 
    border-radius: 10px;
    background-color: rgba(15, 32, 39, 0.3);
}

.nav_tabs ul{
    list-style: none;
}

.nav_tabs ul li{
    float: left;
    display: block !important;
}

.nav_tabs label{
    width: 190px;
    padding: 10px;
    font-weight: 500;
    display: block;
    color: #686868;
    border: 1px solid #686868;
    cursor: pointer;
    text-align: center;
    border-radius: 30px;
    margin: 20px 5px 40px 5px;
    transition: 1s;
}
 
.rd_tabs:checked ~ label{
    color: #cef3f5;
    font-weight: 500;
    border: 1px solid #cef3f5;
}

.rd_tabs:checked ~ .content{
    display: block;
    opacity: 1;
    z-index: 1;
}

.rd_tabs{
    display: none;
}

.content{
    padding-left: 30px;
    opacity: 0;
    transition: 1s;
    position: absolute;
    left: 0;
    z-index: 0;
}

.botao{
    position: absolute;
    border: 1px solid #686868;
    background-color: rgba(15, 32, 39, 0.3);;
    color: #686868;
    width: 760px;
    height: 180px;
    border-radius: 10px;
    padding: 20px;
    font-weight: 500;
    font-size: larger;
    transition-duration: 0.4s;
    cursor: pointer;
    text-align: left;
}

.caixa_img{
    background-size: cover;
    width: 110px;
    height: 110px;
    float: left !important;
    border-radius: 10px;
    margin: 10px 20px 10px 0px;
}

.botao:hover {
    color: #cef3f5;
    border: 1px solid #cef3f5;
}

.filtro{
    width: 240px;
    height: 70%;
    float: right !important;
    background-color: rgba(15, 32, 39, 0.3);
    margin: 120px 30px 150px 50px;
    border: 1px solid #686868;
    border-radius: 10px;
    padding: 20px;
}


.bfiltros:checked ~ label{
    color: #cef3f5;
    font-weight: 500;
    border: 1px solid #cef3f5;
}

.bfiltros{
    display: none;
    margin-bottom: 0px !important;
}

.titulo{
    font-weight: 500;
    font-size: xx-large;
    color: white;
    margin-bottom: 15px;
}

.titulo_filtro{
    font-weight: 500;
    font-size: larger;
    color: white;
    margin-left: 30px;
}

.barra{
    height: 5px;
    background-color: #cef3f5;
    margin-top: 10px;
    margin-left: 130px;
    width: 580px;
}

.barra3{
    border: 1px solid #cef3f5;
    background-color: rgba(15, 32, 39, 0.3);;
    margin-bottom: 10px;
}

.infobarra{
    font-size: small;
    color: #cef3f5;
    margin-bottom: 10px;
}

.infos{
    font-size: medium;
}

.glyphicon-tags{
    float: left;
    color: white;
    font-size: 20px;
}

.circles{
    position: absolute;
    top: 60px;
    left: 0;
    width: 100%;
    height: 940px;
    overflow: hidden;
}

.circles li{
    position: absolute;
    display: block;
    list-style: none;
    width: 20px;
    height: 20px;
    background: rgba(255, 255, 255, 0.2);
    animation: animate 25s linear infinite;
    bottom: -150px;
}


.circles li:nth-child(1){
    left: 25%;
    width: 80px;
    height: 80px;
    animation-delay: 0s;
}


.circles li:nth-child(2){
    left: 5%;
    width: 120px;
    height: 120px;
    animation-delay: 2s;
    animation-duration: 12s;
}

.circles li:nth-child(3){
    left: 70%;
    width: 20px;
    height: 20px;
    animation-delay: 4s;
}

.circles li:nth-child(4){
    left: 40%;
    width: 60px;
    height: 60px;
    animation-delay: 0s;
    animation-duration: 18s;
}

.circles li:nth-child(5){
    left: 65%;
    width: 20px;
    height: 20px;
    animation-delay: 0s;
}

.circles li:nth-child(6){
    left: 75%;
    width: 110px;
    height: 110px;
    animation-delay: 3s;
}

.circles li:nth-child(7){
    left: 35%;
    width: 150px;
    height: 150px;
    animation-delay: 7s;
}

.circles li:nth-child(8){
    left: 50%;
    width: 25px;
    height: 25px;
    animation-delay: 15s;
    animation-duration: 45s;
}

.circles li:nth-child(9){
    left: 20%;
    width: 30px;
    height: 30px;
    animation-delay: 2s;
    animation-duration: 35s;
}

.circles li:nth-child(10){
    left: 85%;
    width: 150px;
    height: 150px;
    animation-delay: 0s;
    animation-duration: 11s;
}



@keyframes animate {

    0%{
        transform: translateY(0) rotate(0deg);
        opacity: 1;
        border-radius: 20%;
    }

    100%{
        transform: translateY(-960px) rotate(720deg);
        opacity: 0;
        border-radius: 30%;
    }

}


