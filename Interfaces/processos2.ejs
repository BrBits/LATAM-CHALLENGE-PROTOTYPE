            :root{
    --back_grande: #131726;
    --back_caixa: rgb(6, 11, 20);
    --form_color: black;
    --form_border: #686868;
    --form_text: #cef3f5;
    --link_color: rgb(19, 159, 214);
}

#logo2{
    height: 60px;
    padding-right: 20px;
    background-color: var(--back_grande);
}

#logo2:hover{
    cursor: pointer;
    background-color: var(--back_grande);
}

#menus{
    list-style-type: none;
    color: var(--form_text);
    overflow: hidden;
    background-color: var(--back_grande);
}

#menus li{
    float: left;
    height: 60px;
    padding: 55px;
    padding-top: 15px;
    padding-bottom: 20px;
    transition: 0.3s;
}

#menus li:hover{
    text-decoration: none;
    background-color: var(--form_border);
    cursor: pointer;
}

*{
    margin: 0;
    padding: 0;
}

body{
    background-image: linear-gradient(to right, #0f2027, #12252c,
     #152931, #192e37, #1c333c,
      #1e3741, #203c47, #22404c,
       #244553, #274959, #294e60, #2c5367) !important; 
    height: 100%;
    width: 100%;
}

.nav_tabs{
    width: 1100px;
    height: 850px;
    margin: 30px auto;
    position: relative; 
    border-radius: 30px;
    background-color: rgba(15, 32, 39, 0.3);
}

.nav_tabs ul{
    list-style: none;
}

.nav_tabs ul li{
    float: left;
    display: block !important;
}

.nav_tabs label{
    width: 200px;
    padding: 10px;
    font-weight: 600;
    display: block;
    color: #686868;
    border: 1px solid #686868;
    cursor: pointer;
    text-align: center;
    border-radius: 30px;
    margin: 20px 5px 40px 0px;
}
 
.rd_tabs:checked ~ label{
    color: #cef3f5;
    font-weight: 600;
    border: 1px solid #cef3f5;
}

.rd_tabs:checked ~ .content{
    display: block;
    opacity: 1;
    z-index: 1;
}

.rd_tabs{
    display: none;
    transition: 1s;
}

.rd_tabs ~ label{
    transition: 1s;
}

.content{
    padding-left: 30px;
    display: block;
    position: absolute;
    left: 0;
    opacity: 0;
    transition: 1s;
    z-index: 0;
}

.botao{
    position: absolute;
    border: 1px solid #686868;
    background-color: rgba(15, 32, 39, 0.3);
    color: #686868;
    width: 760px;
    height: 180px;
    border-radius: 10px;
    padding: 20px;
    font-weight: 500;
    font-size: larger;
    transition-duration: 0.4s;
    cursor: pointer;
    text-align: left;
}

.caixa_img{
    background-size: cover;
    width: 110px;
    height: 110px;
    float: left !important;
    border-radius: 10px;
    margin: 10px 20px 10px 0px;
}

.botao:hover {
    background-color: #0f2027;
    color: #cef3f5;
    border: 1px solid #cef3f5;
}

.filtro{
    width: 240px;
    height: 70%;
    float: right !important;
    background-color: rgba(15, 32, 39, 0.3);
    margin: 120px 30px 150px 50px;
    border: 1px solid #686868;
    border-radius: 10px;
    padding: 20px;
}


.bfiltros:checked ~ label{
    color: #cef3f5;
    font-weight: 600;
    border: 1px solid #cef3f5;
    transition: 0.5s;
}

.bfiltros{
    display: none;
    margin-bottom: 0px !important;
    transition: 0.5s;
}

.bfiltros ~ label{
    transition: 0.5s;
}

.titulo{
    font-weight: 500;
    font-size: xx-large;
    color: white;
    margin-bottom: 15px;
}

.titulo_filtro{
    font-weight: 500;
    font-size: larger;
    color: white;
}

.barra{
    height: 5px;
    background-color: #cef3f5;
    margin-top: 10px;
    margin-left: 130px;
    width: 580px;
}

.barra3{
    border: 1px solid #cef3f5;
    background-color: #0f2027;
    margin-bottom: 10px;
}

.infobarra{
    font-size: small;
    color: #cef3f5;
    margin-bottom: 10px;
}

.infos{
    font-size: medium;
}





